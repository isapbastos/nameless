<%- include("header") -%>

  <div class="box" id="heading">
    <h1> <%= listTitle %> </h1>
  </div>

  <div class="box">
    <% newListUsers.forEach(function(user){ %>
      <form action="/delete" method="post">
      <div class="user">
    
      <input type="checkbox" name="checkbox" value="<%=user._id%>" onChange="this.form.submit()">
        <p><%= user.idPublica  %></p>  <p><%= user.idPrivada  %></p> <p><%= user.medicoes  %></p>
        
       
      
      </div>
      <input type="hidden" name="listName" value="<%= listTitle %>"></input>
      </form>
      <% }); %>
      <form class="user" action="/" method="post">
        <input type="text" name="newUser" placeholder="New User" autocomplete="off">
        <button type="submit" name="list" value="<%= listTitle %>">+</button>
      </form>
  </div>

<%- include("footer") -%>
